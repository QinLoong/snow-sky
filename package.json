{
  "name": "snow-sky",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/umd/snow-sky.umd.cjs",
  "module": "./dist/es/snow-sky.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/snow-sky.js",
      "require": "./dist/umd/snow-sky.umd.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build-only && npm run move-style",
    "preview": "vite preview",
    "build-only": "run-p build-es build-umd",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "move-style": "move-file dist/es/index.css dist/index.css",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5",
    "axios": "^1.3.4",
    "lodash-es": "^4.17.21",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.3",
    "@vue-macros/volar": "^0.8.1",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^21.1.0",
    "move-file-cli": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^6.0.1",
    "typescript": "~4.7.4",
    "unplugin-vue-macros": "^1.8.1",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^3.8.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vue-devtools": "^7.0.17",
    "vitepress": "^1.0.1",
    "vitest": "^0.29.8",
    "vue-tsc": "^1.0.12"
  }
}
